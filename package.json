{
  "name": "@paolostyle/eslint-config",
  "version": "4.0.0",
  "description": "Airbnb based ESLint config for React with Prettier formatting",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/paolostyle/eslint-config"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "author": "Paweł Dąbrowski <dabrowskip9@gmail.com>",
  "license": "MIT",
  "scripts": {
    "check:unused": "eslint-find-rules -u",
